{
    "name": "ProActive",
    "version": "1.0.10",
    "description": "The greatest productivity extension ever!",
    "manifest_version": 3,
    "authors": "Kevin, Jasmine, Brianna, Satwika",
    "permissions": [
        "storage",
        "activeTab"
      ],    
    "host_permissions": ["https://api.openai.com/*"],
    "action":{
        "default_popup": "client/index.html",
        "default_title": "ProActive"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://api.openai.com/"
    }
}